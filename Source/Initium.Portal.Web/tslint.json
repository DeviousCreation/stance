{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-unused-expression-chai": true,
    "quotemark": {
      "options": "single"
    },
    "prefer-for-of": false,
    "whitespace": [true, "check-module"],
    "semicolon": [true, "always"]
  }
}